    <script>
        const person = {
        name: '佐科姐姐',
        age: 1000000,
        address: {
            city: 'ChongQing',
            area: 'NanShan'
        },
        title: ['student',{year:2022, title:'GoodStudent'}]
         }
         const age=person;
         console.log(age);
         const {name: aneme}=person;
         console.log(aneme);
         const {address: {area}}=person;
         console.log(area)
         const {address: {city}}=person;
         console.log(city)
         const {title:[titleOne]}=person;
         console.log(titleOne)

    </script>
</body>
