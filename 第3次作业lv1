<body>
    <script>
        //循环
        function factorial(n){
            var i=1;
            sum=1;
            while(i<=n){
                sum*=i;
                i++;
            }
            return sum
        }
        //递归
        function factorial2(n){
            if(n===1){
                return 1
            }else{
                return n*factorial2(n-1)
            }
        }
        var a=factorial2(10)
        console.log(a)
        
    </script>
</body>
</html>
